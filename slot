import sys
import random

p = ["ğŸ’", "ğŸ‹", "ğŸŠ", "ğŸ””", "7", "ğŸ’"]

def spin_slot():
    return [random.choice(p) for _ in range(3)]

def result_slot(slot):
    if len(set(slot)) == 1:
        return "ã‚¸ãƒ£ãƒƒã‚¯ãƒãƒƒãƒˆ!!!!!!" 
    elif len(set(slot)) == 2:
        return "å½“ãŸã‚Š!"
    else:
        return "ãƒã‚ºãƒ¬"

def number_of_time():
    try:
        
        spins = int(sys.stdin.read().strip())
        if 1 <= spins <= 10:
            return spins
        else:
            sys.exit(1)  
    except (ValueError, TypeError):
        sys.exit(1)  

def play_slot():
    spins = number_of_time()

    for _ in range(spins):
        slot = spin_slot()
        result = result_slot(slot)
        print(f"{' '.join(slot)} {result}")

if __name__ == "__main__":
    play_slot()
